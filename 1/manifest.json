{
    "manifest_version": 3,
    "name": "Right-Sidebar Extension",
    "version": "1.0",
    "icon": "hello_extensions.png",
    "description": "Covers 20% of the right side of the screen",
    "permissions": ["activeTab"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }
  